#NOTE: This file has been generated by ChatGPT Mar 14 Version
import sys

def fibonacci(n):
    if n <= 0:
        return [] #HACK: The list is empty
    elif n == 1:
        return [0]
    elif n == 2:
        return [0, 1]
    else:
        sequence = fibonacci(n-1) #NOTE: Using fibonacci recursively
        sequence.append(sequence[-1] + sequence[-2])
        return sequence

if __name__ == '__main__':
    if len(sys.argv) == 2:
        n = int(sys.argv[1])
        fibonacci_sequence = fibonacci(n)
        print(fibonacci_sequence)
    else:
        print('Usage: python fibonacci.py <n>') #TODO: Improve help
